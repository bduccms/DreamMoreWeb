  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css" />

  <style>
    body {
      background-color: #121212;
      color: white;
    }

    .card {
      position: relative;
      overflow: hidden;
      background-color: #1e1e1e;
      border: none;
      border-radius: 12px;
    }

    .card-body {
      text-align: center;
    }

    .card-img-top {
      border: 4px solid #010318;
      background-color: #000000;
     
    }

    .social-icons {
      position: absolute;
      top: 50%;
      right: -60px;
      transform: translateY(-50%);
      display: flex;
      flex-direction: column;
      gap: 10px;
      transition: left 0.3s ease;
      z-index: 10;
    }

    .card:hover .social-icons {
      right: 10px;
    }

    .social-icons a {
      background-color: #101820;
      color: #fff;
      padding: 6px;
      border-radius: 50%;
      text-align: center;
      width: 35px;
      height: 35px;
      display: flex;
      align-items: center;
      justify-content: center;
      text-decoration: none;
      transition: background-color 0.3s;
    }

    .social-icons a:hover {
      background-color: #ff7800;
      color: #101820;
    }

    .section-title {
      text-align: center;
      margin-bottom: 40px;
      color: #010318;
    }
  </style>
</head>
